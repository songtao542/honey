<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="animateExpand" format="boolean" />
        <attr name="indicatorId" format="reference" />

    </declare-styleable>


    <declare-styleable name="ExpandableLayout_Layout">
        <attr name="layout_showAtTop" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandableHeader">
        <attr name="expansion_layout" format="reference" />
        <attr name="expansion_toggleOnClick" format="boolean" />
        <attr name="expansion_headerIndicator" format="reference" />
        <attr name="expansion_headerIndicatorRotationExpanded" format="integer" />
        <attr name="expansion_headerIndicatorRotationCollapsed" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandableNestedScrollView">
        <attr name="expansion_expanded" format="boolean" />
    </declare-styleable>

    <attr name="expansion_openOnlyOne" format="boolean" />

    <declare-styleable name="ExpandableFrameLayout">
        <attr name="expansion_openOnlyOne" />
    </declare-styleable>
    <declare-styleable name="ExpandableLinearLayout">
        <attr name="expansion_openOnlyOne" />
    </declare-styleable>
    <declare-styleable name="ExpandableRelativeLayout">
        <attr name="expansion_openOnlyOne" />
    </declare-styleable>
    <declare-styleable name="ExpandableConstraintLayout">
        <attr name="expansion_openOnlyOne" />
    </declare-styleable>
</resources>